  "api_examples": {
    "get_matches": {
      "description": "Get all matches from a specific tournament",
      "endpoint": "https://lol.fandom.com/api.php",
      "params": {
        "action": "cargoquery",
        "format": "json",
        "tables": "ScoreboardGames",
        "fields": "Team1,Team2,Winner,DateTime_UTC,OverviewPage",
        "where": "OverviewPage='LCK/2024_Season/Spring_Season'",
        "limit": "500"
      }
    },
    "get_rosters": {
      "description": "Get team rosters for efficient player inference",
      "endpoint": "https://lol.fandom.com/api.php",
      "params": {
        "action": "cargoquery",
        "format": "json",
        "tables": "Rosters",
        "fields": "Team,Player,Role,Start,End",
        "where": "Team='T1'",
        "limit": "500"
      }
    }
  },
